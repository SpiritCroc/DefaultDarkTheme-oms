<?xml version="1.0" encoding="utf-8"?>
<resources>

    <color name="notification_material_background_color">#ff212121</color>

    <bool name="config_allowNotificationIconTextTinting">false</bool>

    <bool name="config_useDarkBgNotificationIconTextTinting">true</bool>

    <color name="system_notification_accent_color">#ff78909c</color>

    <!--
    <drawable name="notification_template_icon_bg">#3333B5E5</drawable>
    <drawable name="notification_template_icon_low_bg">#0cffffff</drawable>
    <drawable name="notification_template_divider">#29000000</drawable>
    <drawable name="notification_template_divider_media">#29ffffff</drawable>
    -->

    <color name="notification_default_color">#ffffffff</color>
    <!--
    <color name="notification_icon_default_color">@color/notification_default_color</color>
    -->

    <color name="notification_progress_background_color">@*android:color/secondary_text_material_dark</color>

    <color name="notification_action_list">#ff303030</color>
    <color name="notification_action_list_dark">#ff303030</color>

    <color name="notification_text_default_color">@*android:color/primary_text_material_dark</color>

    <color name="sender_text_color">@android:color/white</color>

    <style name="TextAppearance.Material.Notification" parent="@*android:style/TextAppearance.Material">
        <item name="android:textColor">@*android:color/secondary_text_material_dark</item>
        <item name="android:textSize">@*android:dimen/notification_text_size</item>
    </style>

    <style name="TextAppearance.Material.Notification.Title">
        <item name="android:textColor">@*android:color/primary_text_default_material_dark</item>
        <item name="android:textSize">@*android:dimen/notification_title_text_size</item>
    </style>

    <style name="TextAppearance.Material.Notification.Info">
        <item name="android:textColor">@*android:color/secondary_text_default_material_dark</item>
        <item name="android:textSize">@*android:dimen/notification_subtext_size</item>
    </style>
</resources>
